{
  "name": "@blrandel/maas-ui-legacy",
  "version": "0.1.12",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/canonical-web-and-design/maas-ui.git"
  },
  "bugs": "https://github.com/canonical-web-and-design/maas-ui/issues",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config webpack.prod.js",
    "build-dev": "NODE_ENV=development webpack --config webpack.dev.js",
    "clean": "rm -rf node_modules build",
    "clean-build": "rm -rf build",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint ./src/app/",
    "lint-package-json": "npmPkgJsonLint .",
    "prettier": "prettier --write 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "prettier-check": "prettier --check 'src/**/*.{js,scss}' '!**/build.scss' '!**/*-min.js'",
    "prepublishOnly": "yarn build",
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js --port 8402 --host 0.0.0.0 --disable-host-check",
    "test": "jest",
    "watch": "NODE_ENV=development webpack --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@types/prop-types": "15.7.3",
    "angular-mocks": "1.7.9",
    "babel-jest": "25.5.1",
    "babel-loader": "8.1.0",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.3",
    "dotenv-flow-webpack": "1.0.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-plugin-react": "7.19.0",
    "html-loader-jest": "0.2.1",
    "html-webpack-plugin": "4.3.0",
    "jest": "25.5.4",
    "jest-fetch-mock": "3.0.3",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.0",
    "npm-package-json-lint": "5.1.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "raw-loader": "4.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sass-loader": "8.0.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@canonical/maas-ui-shared": "0.1.1",
    "@sentry/browser": "5.15.5",
    "@sentry/integrations": "5.15.5",
    "angular": "1.7.9",
    "angular-cookies": "1.7.9",
    "angular-route": "1.7.9",
    "angular-sanitize": "1.7.9",
    "angular-vs-repeat": "1.1.11",
    "babel-polyfill": "6.26.0",
    "cross-env": "7.0.2",
    "jquery": "3.5.0",
    "js-cookie": "2.2.1",
    "macaroon-bakery": "0.2.1",
    "ng-tags-input": "3.2.0",
    "single-spa-angularjs": "^3.2.0",
    "vanilla-framework": "2.0.1"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "resolutions": {
    "lodash": "4.17.15"
  }
}
